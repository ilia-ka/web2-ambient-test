AMBIENT_ENABLED=1
AMBIENT_API_URL=https://api.ambient.xyz/v1/chat/completions
AMBIENT_API_KEY=your_ambient_api_key_here
AMBIENT_MODEL=zai-org/GLM-4.6
AMBIENT_PROMPT_FILE=prompt.txt
AMBIENT_RECEIPT_SAVE=1
AMBIENT_RECEIPT_DIR=data

# OpenAI direct (optional):
OPENAI_ENABLED=0
OPENAI_API_URL=https://api.openai.com/v1/chat/completions
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini
# OPENAI_MODELS=gpt-4o-mini,gpt-4o
# OPENAI_MODEL_GPT_4O_MINI_ENABLED=1
# OPENAI_MODEL_GPT_4O_ENABLED=0

# OpenRouter (optional):
OPENROUTER_ENABLED=0
OPENROUTER_API_URL=https://openrouter.ai/api/v1/chat/completions
OPENROUTER_API=your_openrouter_api_key_here
# OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_MODELS=openai/gpt-5.2,deepseek/deepseek-v3.2,google/gemini-2.5-flash,anthropic/claude-sonnet-4.5
# Per-model toggles (1=on, 0=off):
OPENROUTER_MODEL_OPENAI_GPT_5_2_ENABLED=1
OPENROUTER_MODEL_DEEPSEEK_DEEPSEEK_V3_2_ENABLED=0
OPENROUTER_MODEL_GOOGLE_GEMINI_2_5_FLASH_ENABLED=0
OPENROUTER_MODEL_ANTHROPIC_CLAUDE_SONNET_4_5_ENABLED=0
